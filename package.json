{
  "name": "@jbrowse/mobx-state-tree",
  "version": "5.4.2",
  "description": "Fork of mobx-state-tree from v5.4.2 for use in jbrowse",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist  lib",
    "prebuild": "yarn clean",
    "build": "tsc && cpr lib dist --filter=\\.js$ && tsdown src/index.ts --format esm,cjs --dts",
    "test": "vitest",
    "test-ci": "yarn test:dev && yarn test:prod",
    "test:dev": "cross-env NODE_ENV=development vitest --run",
    "test:prod": "cross-env NODE_ENV=production vitest --run",
    "tag-new-version": "yarn version && git push --tags",
    "deduplicate": "yarn-deduplicate -s fewer yarn.lock",
    "build-docs": "yarn fix-typedoc && rimraf ./docs/API && typedoc --options ./typedocconfig.js",
    "publish-docs": "yarn build-docs && cd ./website && GIT_USER=jamonholmgren USE_SSH=true yarn publish-gh-pages",
    "start": "cd website && yarn start",
    "prepare": "husky install",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "fix-typedoc": "rimraf ./node_modules/typedoc/node_modules/typescript",
    "prettier:list": "prettier --list-different \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "prettier:write": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\""
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-state-tree.git"
  },
  "author": "Michel Weststrate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx-state-tree/issues"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "^12.0.2",
    "concat": "^1.0.3",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "mobx": "^6.3.0",
    "prettier": "^2.4.1",
    "rimraf": "^6.1.2",
    "rollup": "^2.18.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^9.0.1",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^6.1.0",
    "spec.ts": "^1.1.3",
    "ts-node": "^8.10.2",
    "tsdown": "^0.16.8",
    "tslib": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "0.15.8",
    "typedoc-plugin-markdown": "2.2.11",
    "typescript": "^5.9.3",
    "vitest": "^3.2.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "peerDependencies": {
    "mobx": "^6.3.0"
  },
  "keywords": [
    "mobx",
    "mobx-state-tree",
    "promise",
    "reactive",
    "frp",
    "functional-reactive-programming",
    "state management"
  ],
  "gitHead": "27ec7ac0b0743a367fb01a7f40192f3042bd91f2",
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": false,
    "trailingComma": "none",
    "semi": false
  }
}
