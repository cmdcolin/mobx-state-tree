{
  "name": "@jbrowse/mobx-state-tree",
  "version": "5.4.5",
  "description": "Fork of mobx-state-tree starting from v5.4.2 for use in jbrowse 2",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/mobx-state-tree.js",
  "module": "dist/mobx-state-tree.module.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "format": "yarn prettier --write .",
    "build": "tsc && cpr lib dist --filter=\\.js$ && rollup -c",
    "test": "vitest",
    "test-ci": "yarn test:dev && yarn test:prod",
    "test:dev": "cross-env NODE_ENV=development vitest --run",
    "test:prod": "cross-env NODE_ENV=production vitest --run",
    "preversion": "yarn build && yarn test-ci",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdcolin/mobx-state-tree.git"
  },
  "author": "Michel Weststrate",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "^24.10.1",
    "concat": "^1.0.3",
    "cpr": "^3.0.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "mobx": "^6.15.0",
    "prettier": "^3.7.1",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "rollup-plugin-replace": "^2.2.0",
    "spec.ts": "^1.1.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "mobx": "^6.3.0"
  },
  "keywords": [
    "mobx",
    "mobx-state-tree",
    "promise",
    "reactive",
    "frp",
    "functional-reactive-programming",
    "state management"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
